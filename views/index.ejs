<%-include('partials/header')-%>
<h1>index page</h1>
<% blogs.forEach(function(blog){%>
    <div>
        <h2><%=blog.title%></h2>
    </div>
    <div>
        <img src="<%=blog.image%>" alt="">
    </div>
    <p><%=blog.body%></p>
    <span><%=blog.created%></span>

<%})%>
<%-include('partials/footer')-%>